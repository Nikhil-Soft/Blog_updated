<h1 class="text-danger">All Parts</h1>

<table cellpadding="0" cellspacing="0" border="1px" bgcolor="#eee" width="200px">
	<thead>
		<tr>
			<th>SN</th>
			<th>Part Number</th>

		</tr>
	</thead>
	<tbody>
		<% @parts.each do |part| %>
			<tr>
				<td><strong><%= part.id %></strong></td>
				<td><%= part.part_number %></td>
				<td><%= link_to 'Show', part_path(part) %></td>
			</tr>
		<%end%>
	</tbody>
</table>