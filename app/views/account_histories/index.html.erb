<h1 class="text-danger">All Account History</h1>

<table cellpadding="0" cellspacing="0" border="1px" bgcolor="#eee" width="200px">
	<thead>
		<tr>
			<th>SN</th>
			<th>Name</th>
			<th>Show</th>
		</tr>
	</thead>
	<tbody>
		<% @account_histories.each do |account_history| %>
			<tr>
				<td><strong><%= account_history.id %></strong></td>
				<td><%= account_history.credit_rating %></td>
				<td><%= link_to 'Show', account_history_path(account_history) %></td>
			</tr>
		<%end%>
	</tbody>
</table>