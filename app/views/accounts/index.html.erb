<h1 class="text-danger">All Suppliers</h1>

<table cellpadding="0" cellspacing="0" border="1px" bgcolor="#eee" width="200px">
	<thead>
		<tr>
			<th>SN</th>
			<th>Account Nubmer</th>

		</tr>
	</thead>
	<tbody>
		<% @accounts.each do |account| %>
			<tr>
				<td><strong><%= account.id %></strong></td>
				<td><%= account.account_number %></td>
				<td><%= link_to 'Show', account_path(account) %></td>
			</tr>
		<%end%>
	</tbody>
</table>