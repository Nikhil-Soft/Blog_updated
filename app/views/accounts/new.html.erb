
<%= form_with model: @account do |form| %>

	<p>
		<%= form.label :Holder_name %>
		<%= form.select :supplier_id, options_for_select(Supplier.all.map{|b| [b.name, b.id]}) %>
	</p>

	<p>
		<%= form.label :Account_Number %>
		<%= form.text_field :account_number %>
	</p>

	<p><%= form.submit %></p>
<%end%>

<p><%= link_to 'Back', accounts_path %></p>